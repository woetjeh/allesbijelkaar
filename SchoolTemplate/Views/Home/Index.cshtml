@using SchoolTemplate.Database;
@model List<Product>
@{
  ViewData["Title"] = "Home Page";
}

<h1>HD Festivalterrein</h1>

<div class="text-center">
  <h2 class="display-4">Welkom</h2>
  <p>
      Welkom op HD Festivalterrein. Wij organiseren allerlei verschillende soorten festivals, van muziekfestivals tot demonstraties.
  </p>
    <h2>Huidig evenement</h2>
    <p>De gouwe ouwen - 24-30 september 2020</p>
  <ul>
    @foreach (Product p in Model)
    {
      <li>
        @p.Naam @p.Prijs euro
      </li>
    }
  </ul>
</div>
